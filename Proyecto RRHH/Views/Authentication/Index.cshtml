@model Proyecto_RRHH__Datos_.Models.Usuarios

@{
    string Mensaje = ViewBag.Message;
}
<div class="limiter">
    <div class="container-login100">
        <div class="wrap-login100">
            <form class="login100-form validate-form" method="post" autocomplete="off">
                <div>
                    <span class="login100-form-title p-b-70">
                        Bienvenido a
                    </span>
                    <div class="Image-Picture">
                        <img src="~/images/Logo Proyecto RRHH.png" class="ImageLogo" alt="AVATAR">
                    </div>
                </div>


                <div class="wrap-input100 validate-input m-t-85 m-b-35" data-validate="Enter username">
                    <input class="input100" type="text" asp-for="Usuario">
                    <span class="focus-input100" data-placeholder="Usuario"></span>
                </div>

                <div class="wrap-input100 validate-input m-b-50" data-validate="Enter password">
                    <input class="input100" type="password" asp-for="Contrasena">
                    <span class="focus-input100" data-placeholder="Contraseña"></span>
                </div>

                <div class="container-login100-form-btn">
                    <button class="login100-form-btn" type="submit">
                        Ingresar
                    </button>
                </div>
                <ul class="login-more p-t-190">
                    <li class="m-b-8">
                        <span class="txt1">
                            ¿Olvidó su
                        </span>

                        <a href="#" class="txt2">
                            Usuario / Contraseña?
                        </a>
                    </li>

                    <li>
                        <span class="txt1">
                            ¿No tienes cuenta?
                        </span>

                        <a href="#" class="txt2">
                            Registráte
                        </a>
                    </li>
                </ul>
            </form>
        </div>
    </div>
</div>


<div id="dropDownSelect1"></div>

